<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="helthe_turbolinks.listener.cross_domain.class">Helthe\Component\Turbolinks\EventListener\CrossDomainListener</parameter>
        <parameter key="helthe_turbolinks.listener.redirect.class">Helthe\Component\Turbolinks\EventListener\RedirectListener</parameter>
        <parameter key="helthe_turbolinks.listener.request_method.class">Helthe\Component\Turbolinks\EventListener\RequestMethodListener</parameter>
    </parameters>

    <services>
        <service id="helthe_turbolinks.listener.cross_domain" class="%helthe_turbolinks.listener.cross_domain.class%">
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="helthe_turbolinks.listener.redirect" class="%helthe_turbolinks.listener.redirect.class%">
            <tag name="kernel.event_subscriber" />
        </service>

        <service id="helthe_turbolinks.listener.request_method" class="%helthe_turbolinks.listener.request_method.class%">
            <tag name="kernel.event_subscriber" />
        </service>
    </services>
</container>
